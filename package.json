{
  "name": "lamps-code-review",
  "version": "0.2.0",
  "description": "AI-powered code review SDK and CLI for modern web codebases",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "lamps-review": "./bin/lamps-review.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "code-review",
    "static-analysis",
    "ai",
    "typescript",
    "react",
    "nextjs",
    "python",
    "cli"
  ],
  "author": "Lamps <https://x.com/lamps_apple>",
  "repository": {
    "type": "git",
    "url": "https://github.com/AppleLamps/lamps-code-review"
  },
  "homepage": "https://github.com/AppleLamps/lamps-code-review#readme",
  "bugs": {
    "url": "https://github.com/AppleLamps/lamps-code-review/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
